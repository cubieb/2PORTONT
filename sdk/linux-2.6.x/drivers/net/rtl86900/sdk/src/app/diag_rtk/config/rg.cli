//rg configuration
//set configuration
//lan-intf configuration
//ip-version configuration
//ip_version 0:IPVER_V4ONLY 1:IPVER_V6ONLY 2:IPVER_V4V6
//gateway-mac configuration
//gmac, A:B:C:D:E:F
//ip-addr configuration
//ip, ipv4 address
//ip-mask configuration
//mask, ipv4 mask
//ipv6_addr configuration
//ipv6_addr, 1234::ABCD
//ipv6_network_mask_length configuration
//ipv6_network_mask_length 
//port-mask configuration
//port_mask, include extension port <0x0-0xfff>
//untag-mask configuration
//untag_mask, not include extension port <0x0-0x7f>
//intf-vlan_id configuration
//intf_vid, <0-4095>
//vlan-based-pri-enable configuration
//enable
//vlan-based-pri configuration
//vlan_based_pri <0-7>
//mtu configuration
//mtu
//isIVL configuration
//isIVL, 0:SVL 1:IVL
rg set lan-intf ip-version <UINT:ip_version> gateway-mac <MACADDR:gmac> ip-addr <IPV4ADDR:ip> ip-mask <IPV4ADDR:mask> ipv6-addr <IPV6ADDR:ipv6_addr> ipv6_network_mask_length <UINT:ipv6_network_mask_length> port-mask <HEX:port_mask> untag-mask <HEX:untag_mask> intf-vlan_id <UINT:intf_vid> vlan-based-pri-enable enable vlan-based-pri <UINT:vlan_based_pri> mtu <UINT:mtu> isIVL <UINT:isIVL>

