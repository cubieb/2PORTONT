//l2 table configuration
//init configuration
l2-table init

//l2 table configuration
//delete configuration
//all unicast MAC entries
//include static entries
l2-table del all { include-static }

//l2 table configuration
//delete configuration
//ipv4 multicast entry
//destination IP configuration
//destination IP
l2-table del ip-mcast dip <IPV4ADDR:dip>

//l2 table configuration
//get configuration
//ipv4 multicast entry
//destination IP configuration
//destination IP
l2-table get ip-mcast dip <IPV4ADDR:dip>

//l2 table configuration
//add configuration
//ipv4 multicast entry
//destination IP configuration
//destination IP
//port configuration
//port list, <0-6>
//all
//none
l2-table add ip-mcast dip <IPV4ADDR:dip> port ( <PORT_LIST:ports> | all | none )

//l2 table configuration
//delete configuration
//multicast l2 entry
//filter id configuration
//fid value, <0-15>
//mac addresss configuration
//multicast MAC address, A:B:C:D:E:F
l2-table del mac-mcast filter-id <UINT:fid> mac-address <MACADDR:mac>

//l2 table configuration
//add configuration
//multicast l2 entry
//filter id configuration
//fid value, <0-15>
//mac addresss configuration
//multicast MAC address, A:B:C:D:E:F
//port configuration
//port list, <0-6>
//all
//none
l2-table add mac-mcast filter-id <UINT:fid> mac-address <MACADDR:mac> port ( <PORT_LIST:ports> | all | none )

//l2 table configuration
//get configuration
//multicast l2 entry
//filter-id configuration
//fid, <0~15>
//mac addresss configuration
//multicast l2 address, A:B:C:D:E:F
l2-table get mac-mcast filter-id <UINT:fid> mac-address <MACADDR:mac>


