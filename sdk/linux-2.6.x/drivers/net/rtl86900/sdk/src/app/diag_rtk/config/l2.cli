//vid value, <0-4095>
//mac addresss configuration
//unicast l2 address, A:B:C:D:E:F
//ingress ctag learning entry
//vlan id configuration
//vlan id, <0~4095>
l2-table add mac-ucast vid <UINT:vid> mac-address <MACADDR:mac> ctag-if vid <UINT:cvid>

//l2 table configuration
//add configuration
//unicast l2 entry
//filter-id configuration
//fid, <0~15>
//mac addresss configuration
//unicast l2 address, A:B:C:D:E:F
//ingress ctag learning entry
//vlan id configuration
//vlan id, <0~4095>
l2-table add mac-ucast filter-id <UINT:fid> mac-address <MACADDR:mac> ctag-if state ( disable | enable )


//l2 table configuration
//add configuration
//unicast l2 entry
//filter-id configuration
//fid, <0~15>
//mac addresss configuration
//unicast l2 address, A:B:C:D:E:F
//ingress ctag learning entry
//vlan id configuration
//vlan id, <0~4095>
l2-table add mac-ucast filter-id <UINT:fid> mac-address <MACADDR:mac> ctag-if vid <UINT:cvid>

//l2 table configuration
//get configuration
//l2 table aging update type while no learning 
//state configuration
l2-table get nolearn-aging state 

//l2 table configuration
//set configuration
//l2 table aging update type while no learning 
//state configuration
//disable
//enable
l2-table set nolearn-aging state ( disable | enable )

//l2 table configuration
//get configuration
//l2 table svl untag vid learning 
l2-table get svl-untag-vid-learn

//l2 table configuration
//set configuration
//l2 table svl untag vid learning 
//ingress-vid
//vid0
l2-table set svl-untag-vid-learn ( ingress-vid | vid0 ) 

//l2 table configuration
//get configuration
//entry configuration
//address configuration
//valid configuration
l2-table get entry address valid

//l2 table configuration
//set configuration
//arp usage as known
//follow age field in lut
//always tread as known
l2-table set arp-usage-as-known ( follow-age | always-known ) 

//l2 table configuration
//get configuration
//arp usage as known
l2-table get arp-usage-as-known
